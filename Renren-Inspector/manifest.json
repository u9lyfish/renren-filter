{
    "content_scripts": [
        {
            "js": [
                "contentscript.js"
            ], 
            "matches": [
                "http://www.renren.com/*"
            ]
        }
    ], 
    "description": "Display news feed types of renren.com homepage in a more user-friendly way", 
    "manifest_version": 2, 
    "name": "Renren Inspector", 
    "permissions": [
        "https://api.github.com/*"
    ], 
    "version": "1.0"
}
