{
    "content_scripts": [
        {
            "js": [
                "contentscripts.js"
            ], 
            "matches": [
                "http://www.renren.com/*"
            ]
        }
    ], 
    "description": "Filter out the specified content from renren.com homepage", 
    "manifest_version": 2, 
    "name": "Renren Filter", 
    "version": "1.0"
}
