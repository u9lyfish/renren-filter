{
    "background": {
        "persistent": false, 
        "scripts": [
            "background.js"
        ]
    }, 
    "description": "Filter out the specified content from renren.com homepage", 
    "manifest_version": 2, 
    "name": "Renren Filter", 
    "permissions": [
        "tabs", 
        "http://www.renren.com/*"
    ], 
    "version": "1.0"
}
